neuron izhikevich_nestml:

  state:
	V_m mV = V_m_init# [[10mV>=V_m >=0mV]]#test for complex  interval expressions
    u real = V_m*b/1mV
    I real = 0
  end

  equations:
    V_m' = 0.04 * V_m*V_m/1mV + 5.0 * V_m + (140 - u + I)
    u' = a*(b*V_m/1mV-u)
  end

  parameters:
    a real
    b real
    c real
    d real
	V_m_init mV
	I_e mA	
  end

  input:
    spikes   <- spike
    currents <- current
  end

  output: spike

  update:
    integrate_odes()
    test mV = a<b?10mV: 20mV

    V_m = spikes*1mV

    if V_m >= 30mV:
      V_m = c*1mV
      emit_spike() 
	end
    I = currents
  end

end
