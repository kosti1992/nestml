neuron CoCoInvariantBlockCorrectlyBuilt:
    state:
        V_m mV = 0mV
    end

    input:
        #empty
    end

    parameters:
        C_m pF = 200pF
    end

    output : spike

    update:
        #empty
    end


    constraints:
       90mV <= V_m <= 0mV # this should throw an error, since interval empty
       500pF <= C_m # this should throw an error, since lower bound under
    end

end